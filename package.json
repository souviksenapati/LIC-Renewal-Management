{
  "name": "lic_app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:testing": "cross-env EXPO_PUBLIC_ENV=testing expo start",
    "start:production": "cross-env EXPO_PUBLIC_ENV=production expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
